<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
 <mapper namespace="com.jgm.roujin.persistence.SalutariumDao">



	<insert id="insertSal" parameterType="salutariumVO" useGeneratedKeys="true" keyProperty="sequence">
	
		INSERT INTO tbl_salutarium(username,name,address,fee,description,feature) 
		VALUES(#{username},#{name},#{address},#{fee},#{description},#{feature})
	
	</insert>
	
 </mapper>